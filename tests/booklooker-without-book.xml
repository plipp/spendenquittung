        <!ELEMENT DetailLinkUrl (#PCDATA)>
        <!ELEMENT Year (#PCDATA)>
        <!ELEMENT Edition (#PCDATA)>
        <!ELEMENT Publisher (#PCDATA)>
        <!ELEMENT PicURL (#PCDATA)>
        <!ELEMENT New (#PCDATA)>
        <!ELEMENT Offerer (#PCDATA)>
        <!ELEMENT OffererId (#PCDATA)>
        <!ELEMENT RatePositive (#PCDATA)>
        <!ELEMENT Condition (#PCDATA)>
